    "dev-classic": "NEXT_TELEMETRY_DISABLED=1 next dev",
    "dev-no-logs": "NEXT_TELEMETRY_DISABLED=1 next dev",
    "build": "NODE_OPTIONS='--max-old-space-size=2048' next build",
