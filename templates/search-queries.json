{
  "searchPatterns": {
    "marketDiscovery": {
      "description": "Queries for discovering market landscape and major players",
      "patterns": [
        "{market} competitive landscape 2024",
        "top {service} companies {geography}",
        "{industry} market leaders analysis",
        "{market} vendor comparison matrix",
        "best {service} providers {region}",
        "{industry} market share leaders",
        "Gartner {market} Magic Quadrant",
        "Forrester {industry} Wave report",
        "{market} startup ecosystem {year}"
      ],
      "variables": {
        "market": "Primary market category (e.g., 'employee benefits', 'CRM software')",
        "service": "Service type (e.g., 'HR technology', 'cloud storage')",
        "industry": "Industry classification (e.g., 'fintech', 'edtech')",
        "geography": "Geographic region (e.g., 'Europe', 'Greece', 'North America')",
        "region": "Specific region (e.g., 'EMEA', 'Asia Pacific')",
        "year": "Current year for recent analysis"
      }
    },
    "competitorSpecific": {
      "description": "Queries for researching specific competitors",
      "patterns": [
        "{company} competitors alternatives",
        "{company} vs {competitor} comparison",
        "companies like {company}",
        "{company} competitive analysis",
        "{company} market position",
        "{company} customer reviews ratings",
        "{company} features pricing plans",
        "{company} case studies testimonials",
        "{company} funding investors valuation",
        "{company} revenue growth employees"
      ],
      "variables": {
        "company": "Specific company name",
        "competitor": "Known competitor for comparison"
      }
    },
